<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd
        "> 

<!-- 	消费方应用名，用于计算依赖关系，不是匹配条件，不要与提供方一样 使用multicast广播注册中心暴露发现服务地址 -->
  	<dubbo:application name="order.maxtp" />    
<!-- 	生成远程服务代理，可以和本地bean一样使用demoService -->
	<dubbo:registry  protocol="zookeeper" address="zookeeper://192.168.6.110:2181" />

	<dubbo:reference id="orderDubboService"
		interface="com.qianqian.order.service.IOrderDubboService" version="1.0"
		check="false" />
	<dubbo:reference id="prdCommentService"
		interface="com.qianqian.comment.service.IProductRateDubboService"
		check="false" />
	<!-- 申请单操作接口 -->
	<dubbo:reference id="applyDubboService"
		interface="com.qianqian.apply.service.IApplyDubboService" version="1.0"
		check="false" />
	<dubbo:reference id="userDubboService"
		interface="com.qianqian.member.service.IUserDubboService" check="false"
		version="1.1" />
	<!-- member.maxtp接口依赖关系配置结束 -->

	<dubbo:reference id="productRateDubboService" interface="com.qianqian.comment.service.IProductRateDubboService" check="false" version="1.0"/>

	<!-- 发货赔偿操作接口 -->
	<dubbo:reference id="orderOvertimeDubboService" 
		interface="com.qianqian.order.service.IOrderOvertimeDubboService" check="false" version="1.0"/>
</beans>